  "content_security_policy": "default-src 'self'; script-src 'self' http://localhost:3000 https://localhost:3000 https://www.gstatic.com wss://*.firebaseio.com https://*.firebaseio.com https://www.googleapis.com 'unsafe-eval'; connect-src http://localhost:3000 https://localhost:3000 https://www.gstatic.com wss://*.firebaseio.com ://*.firebaseio.com https://www.googleapis.com; style-src * 'unsafe-inline' 'self' blob:; img-src 'self' data:;",
  "oauth2": {
    "client_id": "211197224104-c9cmdoi2o1bd60c8l6a6l0smbg9ilkc8.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "key": ""
}
